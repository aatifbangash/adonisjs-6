  <script>

    $(document).ready(function () {
      if ($(window).width() > 991) {
        $(".nav-item.dropdown").on("mouseenter click", function () {
          $(".dropdown-menu").toggleClass("d-none d-block");
        });
      } else {
        $(".nav-item.dropdown").on("click", function () {
          $(".dropdown-menu").toggleClass("d-none d-block");
        });
      }
    });
  
// List of available blog URLs
    var blogURLs = ["camp-lejeune","cpap", "lawsuit-blog", "paraquat", "roundup-lawsuit"];
    var matchingURLs = [];
    var debounceTimer;
    var suggestionsContainer = document.getElementById('suggestionsContainer');

    // Add a click event listener to the document
    document.addEventListener('click', function (event) {
      var isClickInsideElement = suggestionsContainer.contains(event.target);

      if (!isClickInsideElement) {
        suggestionsContainer.innerHTML = "";
      }
    });

    function showSuggestions() {

        clearTimeout(debounceTimer);
        var searchQuery = document.getElementById('searchInput').value.toLowerCase();
        

        debounceTimer = setTimeout(function() {
          $.ajax({ 
            url: '/search', 
            data: { query: searchQuery }, 
            method: 'GET', 
            success: function(response) { 
              suggestionsContainer.innerHTML = "";
              if(response) {
                matchingURLs = response.map(object => object.title);
                var suggestionsHTML = matchingURLs.map(url => {
                  const highlightedText = highlightMatchingText(url, searchQuery);
                  return `<div style="margin-left: 12px;" onclick="selectSuggestion('${url}')"><a href="#">${highlightedText}</a></div>`
                }).join('');
                suggestionsContainer.innerHTML = suggestionsHTML;
              }
            } 
          });
        }, 500);
    }

    function highlightMatchingText(text, query) {
      const lowercasedText = text.toLowerCase();
      const index = lowercasedText.indexOf(query);
      
      if (index !== -1) {
        const highlightedText = text.substring(0, index) +
          '<strong>' +
          text.substring(index, index + query.length) +
          '</strong>' +
          text.substring(index + query.length);

        return highlightedText;
      }

      return text;
    }

    function selectSuggestion(url) {
        return null
        // Set the selected suggestion as the search input value
        document.getElementById('searchInput').value = url;

        // Clear the suggestions container
        document.getElementById('suggestionsContainer').innerHTML = '';

        // Navigate to the respective URL or 'coming_soon' if there is no match
        window.location.href = matchingURLs.length > 0 ? `${url}.html` : 'Coming_soon/index.html';
    }

    function performSearch() {
        var searchQuery = document.getElementById('searchInput').value;
        console.log("Search Query: " + searchQuery);
    }
  </script>

